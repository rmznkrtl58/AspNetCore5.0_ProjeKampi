@model List<EntityLayer.Concrete.AppUser>
@{
    ViewData["Title"] = "userrolelist";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <div class="ibox ">
            <div class="ibox-title">
                <h5>Rol Atamaları</h5>
                <div class="ibox-tools">
                    <a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>
                </div>
            </div>
            <div class="ibox-content">
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>Kullanıcı Adı</th>
                            <th>Rol Ata</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var x in Model)
                        {
                            
                            <tr>
                                <td>@x.Id</td>
                                <td>@x.UserName</td>
                                <td><a href="/Admin/AdminRole/AssingRole/@x.Id" class="btn btn-outline-primary">Atama Yap</a></td>
                            </tr>
                        }
                    </tbody>
                </table>
                <a href="/Admin/AdminRole/Index/" class="btn btn-outline-info">Rol Listesine Git</a>
            </div>
        </div>
    </div>
</div>





